{
  "dependencies": {
    "@fontsource/jersey-10": "^5.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "color-convert": "^3.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "regl": "^2.1.1",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "globals": "^16.5.0",
    "pixelarticons": "1.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.0"
  },
  "license": "GPL-3.0-only",
  "name": "game-of-life",
  "private": true,
  "scripts": {
    "build": "tsc -b && vite build",
    "deploy": "scripts/deploy.sh",
    "dev": "vite --port 3000 --host 0.0.0.0",
    "format": "biome format --write",
    "generate": "npm run generate:icons && npm run generate:objects",
    "generate:icons": "npx tsx scripts/fix-pixelarticons.ts",
    "generate:objects": "npx tsx scripts/convert-rles.ts",
    "lint": "biome lint --reporter=summary",
    "start": "vite preview --port 3000  --host 0.0.0.0",
    "tidy": "npm run lint && npm run format"
  },
  "type": "module",
  "version": "0.0.0"
}